enum TipoUsuario {
  Admin
  Jefe_Carrera
  Docente
}

model Usuario {
  id           Int     @id @default(autoincrement())
  username     String  @unique
  email        String? @unique
  hashPassword String

  fechaCreacion     DateTime
  fechaModificacion DateTime
  activo            Boolean

  tipoUsuario TipoUsuario @default(Docente)
}
